{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "object-literal-sort-keys": false
  },
  "rules": {
    "prettier": true,
    "interface-name": [true, "never-prefix"],
    "member-access": [
      false
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "no-console": [
      true,
      "log",
      "debug"
    ],
    "no-var-requires": false,
    "no-namespace": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
